																<LineSegment Point="4,6" />
																<LineSegment Point="10,6" />
																<LineSegment Point="10,0" />
																<LineSegment Point="14,0" />
																<LineSegment Point="14,14" />
																<LineSegment Point="0,14" />
															</PathFigure>
														</PathGeometry>
													</Ellipse.Clip>
												</Ellipse>
												<Rectangle
                                                    Width="2"
                                                    Margin="0,1,0,5"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.QuickAccessIcon>
								</ribbon:RibbonButton>
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="B"
                                    Content="Off"
                                    MinSize="Large"
                                    ToolTip.Tip="Off">
									<ribbon:RibbonButton.LargeIcon>
										<ControlTemplate>
											<Panel Width="32" Height="32">
												<Ellipse Stroke="{Binding $parent[TemplatedControl].Foreground}" StrokeThickness="4" />
												<Rectangle
                                                    Width="4"
                                                    Margin="0,9"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.LargeIcon>
									<ribbon:RibbonButton.QuickAccessIcon>
										<ControlTemplate>
											<Panel Width="16" Height="16">
												<Ellipse Stroke="{Binding $parent[TemplatedControl].Foreground}" StrokeThickness="2" />
												<Rectangle
                                                    Width="2"
                                                    Margin="0,4"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.QuickAccessIcon>
								</ribbon:RibbonButton>
							</ribbon:RibbonGroupBox>
						</ribbon:RibbonTab.Groups>
					</ribbon:RibbonTab>
					<ribbon:RibbonTab ribbon:KeyTip.KeyTipKeys="Q" Header="Test B">
						<ribbon:RibbonTab.Groups>
							<ribbon:RibbonGroupBox
                                ribbon:KeyTip.KeyTipKeys="D1"
                                Command="{Binding OnClickCommand}"
                                Header="Group">
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="Y"
                                    Content="Yes"
                                    MinSize="Large"
                                    QuickAccessIcon="{Binding Icon, RelativeSource={RelativeSource Mode=Self}}"
                                    ToolTip.Tip="Yes">
									<ribbon:RibbonButton.LargeIcon>
										<ControlTemplate>
											<Path
                                                Width="32"
                                                Height="32"
                                                Data="M 1 15 L 10 24 L 30 4"
                                                Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                StrokeThickness="4" />
										</ControlTemplate>
									</ribbon:RibbonButton.LargeIcon>
									<ribbon:RibbonButton.Icon>
										<ControlTemplate>
											<Path
                                                Width="16"
                                                Height="16"
                                                Data="M 0.5 7 L 5 11.5 L 15 1.5"
                                                Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                StrokeThickness="2" />
										</ControlTemplate>
									</ribbon:RibbonButton.Icon>
								</ribbon:RibbonButton>
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="N"
                                    Content="No"
                                    MinSize="Large"
                                    QuickAccessIcon="{Binding Icon, RelativeSource={RelativeSource Mode=Self}}"
                                    ToolTip.Tip="No">
									<ribbon:RibbonButton.LargeIcon>
										<ControlTemplate>
											<Path
                                                Width="32"
                                                Height="32"
                                                Data="M 2 2 L 30 30 M 30 2 L 2 30"
                                                Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                StrokeThickness="4" />
										</ControlTemplate>
									</ribbon:RibbonButton.LargeIcon>
									<ribbon:RibbonButton.Icon>
										<ControlTemplate>
											<Path
                                                Width="16"
                                                Height="16"
                                                Data="M 1 1 L 15 15 M 15 1 L 1 15"
                                                Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                StrokeThickness="2" />
										</ControlTemplate>
									</ribbon:RibbonButton.Icon>
								</ribbon:RibbonButton>
							</ribbon:RibbonGroupBox>
						</ribbon:RibbonTab.Groups>
					</ribbon:RibbonTab>
					<ribbon:RibbonTab ribbon:KeyTip.KeyTipKeys="N" Header="Test C">
						<ribbon:RibbonTab.Groups>
							<ribbon:RibbonGroupBox
                                ribbon:KeyTip.KeyTipKeys="D1"
                                Command="{Binding OnClickCommand}"
                                Header="Test Group lel">
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="A"
                                    Content="Non't"
                                    MinSize="Large"
                                    ToolTip.Tip="Non't">
									<ribbon:RibbonButton.LargeIcon>
										<ControlTemplate>
											<Panel Width="32" Height="32">
												<Ellipse
                                                    Margin="2,4,2,0"
                                                    Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                    StrokeThickness="4">
													<Ellipse.Clip>
														<PathGeometry>
															<PathFigure IsClosed="True" StartPoint="0,0">
																<LineSegment Point="8,0" />
																<LineSegment Point="8,11" />
																<LineSegment Point="19,11" />
																<LineSegment Point="19,0" />
																<LineSegment Point="28,0" />
																<LineSegment Point="28,28" />
																<LineSegment Point="0,28" />
															</PathFigure>
														</PathGeometry>
													</Ellipse.Clip>
												</Ellipse>
												<Rectangle
                                                    Width="4"
                                                    Margin="0,0,0,9"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.LargeIcon>
									<ribbon:RibbonButton.QuickAccessIcon>
										<ControlTemplate>
											<Panel Width="16" Height="16">
												<Ellipse
                                                    Margin="1,2,1,0"
                                                    Stroke="{Binding $parent[TemplatedControl].Foreground}"
                                                    StrokeThickness="2">
													<Ellipse.Clip>
														<PathGeometry>
															<PathFigure IsClosed="True" StartPoint="0,0">
																<LineSegment Point="4,0" />
																<LineSegment Point="4,6" />
																<LineSegment Point="10,6" />
																<LineSegment Point="10,0" />
																<LineSegment Point="14,0" />
																<LineSegment Point="14,14" />
																<LineSegment Point="0,14" />
															</PathFigure>
														</PathGeometry>
													</Ellipse.Clip>
												</Ellipse>
												<Rectangle
                                                    Width="2"
                                                    Margin="0,1,0,5"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.QuickAccessIcon>
								</ribbon:RibbonButton>
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="B"
                                    Content="Yesn't"
                                    MinSize="Large"
                                    ToolTip.Tip="Yesn't">
									<ribbon:RibbonButton.LargeIcon>
										<ControlTemplate>
											<Panel Width="32" Height="32">
												<Ellipse Stroke="{Binding $parent[TemplatedControl].Foreground}" StrokeThickness="4" />
												<Rectangle
                                                    Width="4"
                                                    Margin="0,9"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.LargeIcon>
									<ribbon:RibbonButton.QuickAccessIcon>
										<ControlTemplate>
											<Panel Width="16" Height="16">
												<Ellipse Stroke="{Binding $parent[TemplatedControl].Foreground}" StrokeThickness="2" />
												<Rectangle
                                                    Width="2"
                                                    Margin="0,4"
                                                    Fill="{Binding $parent[TemplatedControl].Foreground}" />
											</Panel>
										</ControlTemplate>
									</ribbon:RibbonButton.QuickAccessIcon>
								</ribbon:RibbonButton>
							</ribbon:RibbonGroupBox>
						</ribbon:RibbonTab.Groups>
					</ribbon:RibbonTab>
				</ribbon:RibbonContextualTabGroup>
				<ribbon:RibbonContextualTabGroup
                    Background="Green"
                    Header="Contextual Group 2"
                    IsVisible="{Binding ShowContextualGroup2}">
					<ribbon:RibbonTab ribbon:KeyTip.KeyTipKeys="T" Header="This tab's name is really long">
						<ribbon:RibbonTab.Groups>
							<ribbon:RibbonGroupBox
                                ribbon:KeyTip.KeyTipKeys="D1"
                                Command="{Binding OnClickCommand}"
                                Header="Another bloody group">
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="A"
                                    Content="Button 1"
                                    Icon="{DynamicResource Icon1}"
                                    LargeIcon="{DynamicResource Icon1Large}"
                                    MinSize="Medium"
                                    QuickAccessIcon="{DynamicResource Icon1QuickAccess}"
                                    ToolTip.Tip="Button 1" />
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="B"
                                    Content="Button 2"
                                    Icon="{DynamicResource Icon2}"
                                    LargeIcon="{DynamicResource Icon2Large}"
                                    MinSize="Medium"
                                    QuickAccessIcon="{DynamicResource Icon2QuickAccess}"
                                    ToolTip.Tip="Button 2" />
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="C"
                                    Content="Button 3"
                                    Icon="{DynamicResource Icon2}"
                                    LargeIcon="{DynamicResource Icon2Large}"
                                    MinSize="Medium"
                                    QuickAccessIcon="{DynamicResource Icon2QuickAccess}"
                                    ToolTip.Tip="Button 3" />
							</ribbon:RibbonGroupBox>
						</ribbon:RibbonTab.Groups>
					</ribbon:RibbonTab>
				</ribbon:RibbonContextualTabGroup>
				<ribbon:RibbonContextualTabGroup
                    Background="Yellow"
                    Header="Contextual Group 3 with a very long name"
                    IsVisible="{Binding ShowContextualGroup3}">
					<ribbon:RibbonTab ribbon:KeyTip.KeyTipKeys="T" Header="lol">
						<ribbon:RibbonTab.Groups>
							<ribbon:RibbonGroupBox
                                ribbon:KeyTip.KeyTipKeys="D1"
                                Command="{Binding OnClickCommand}"
                                Header="lmao">
								<ribbon:RibbonButton
                                    ribbon:KeyTip.KeyTipKeys="N"
                                    Content="no u"
                                    Icon="{DynamicResource Icon1}"
                                    LargeIcon="{DynamicResource Icon1Large}"
                                    MinSize="Small"
                                    QuickAccessIcon="{DynamicResource Icon1QuickAccess}"
                                    ToolTip.Tip="no u" />
							</ribbon:RibbonGroupBox>
						</ribbon:RibbonTab.Groups>
					</ribbon:RibbonTab>
				</ribbon:RibbonContextualTabGroup>
			</windows:DesktopRibbon.Tabs>
		</windows:DesktopRibbon>

		<StackPanel
            Grid.Row="1"
            Margin="25"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Orientation="Vertical"
            Spacing="10">
			<ToggleSwitch
                x:Name="LightsToggleSwitch"
                IsChecked="{Binding SwitchTheme}"
                OffContent="Lights off"
                OnContent="Lights on" />
			<Button
                x:Name="HorizontalRibbonButton"
                Content="Switch Ribbon to horizontal orientation"
                IsVisible="False" />
			<Button x:Name="VerticalRibbonButton" Content="Switch Ribbon to vertical orientation" />
			<StackPanel Orientation="Horizontal">
				<TextBlock Text="LAST ACTION: " />
				<TextBlock Text="{Binding LastActionText}" />
			</StackPanel>
			<StackPanel VerticalAlignment="Center" Orientation="Horizontal">
				<TextBlock
                    VerticalAlignment="Center"
                    FontSize="13"
                    Text="System Decorations Mode : " />
				<ComboBox
                    Width="200"
                    DisplayMemberBinding="{Binding Value}"
                    ItemsSource="{Binding Source={extensions:Enumeration {x:Type enums:SystemDecorations}}}"
                    SelectedValue="{Binding SystemDecorations, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=RibbonWindow}, Mode=TwoWay}"
                    SelectedValueBinding="{Binding Value}" />
			</StackPanel>
			<!--  CheckBox x:Name="LightsOnCheckBox" Content="Lights" IsChecked="True"/  -->
			<CheckBox Content="Show Contextual tab group 1" IsChecked="{Binding ShowContextualGroup1, Mode=OneWay}" />
			<CheckBox Content="Show Contextual tab group 2" IsChecked="{Binding ShowContextualGroup2}" />
			<CheckBox Content="Show Contextual tab group 3, which has a very long name so you can easily see that its only tab's width does not prohibit the group from getting wider as needed" IsChecked="{Binding ShowContextualGroup3}" />
			<!--  Button x:Name="TestItemsButton" Content="Test"/  -->
		</StackPanel>
	</Grid>
</windows:RibbonWindow>